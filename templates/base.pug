    //   release: "my-project-name@X",
    //
    //   // We recommend adjusting this value in production, or using tracesSampler
    //   // for finer control
    //   tracesSampleRate: 1.0,
    // });

  // the missing javascript functions
  script(src="https://unpkg.com/lodash@4.17.21/lodash.min.js")

  // ajax calls
  script(src="https://unpkg.com/axios@0.26.1/dist/axios.min.js")

  // Buefy UI framework
  script(src="https://unpkg.com/buefy@0.9.19/dist/buefy.min.js")
  link(rel="stylesheet", href="https://unpkg.com/buefy@0.9.19/dist/buefy.min.css")
  link(rel="stylesheet", href="//cdn.materialdesignicons.com/2.0.46/css/materialdesignicons.min.css")
  Vue.use(Buefy.default)

  -compress js
